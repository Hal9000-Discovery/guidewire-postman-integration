      "item": [
        {
          "name": "01_Create_Account",
          "item": [
            {
              "name": "v1 POST Create Billing Account",
              "event": [
                {
                  "listen": "test",
                  "script": {
                    "exec": [
                      "// ---------------------------------------------------------\r",
                      "// Test Case: Create Billing Account – Successful Creation\r",
                      "// Purpose:\r",
                      "// - Verify that the BillingCenter API successfully creates\r",
                      "//   a new billing account\r",
                      "// - HTTP 201 confirms a new resource was created\r",
                      "// ---------------------------------------------------------\r",
                      "pm.test(\"Account created (201)\", () => {\r",
                      "  // 201 Created indicates the billing account was persisted successfully\r",
                      "  pm.response.to.have.status(201);\r",
                      "});\r",
                      "\r",
                      "\r",
                      "// ---------------------------------------------------------\r",
                      "// Test Case: Billing Account Identifier Returned\r",
                      "// Purpose:\r",
                      "// - Ensure the response includes a unique accountId\r",
                      "// - Persist accountId for use in downstream API calls\r",
                      "//   (e.g., Get Account, Create Invoice, Post Payment)\r",
                      "// ---------------------------------------------------------\r",
                      "pm.test(\"Account ID returned\", () => {\r",
                      "  // Parse JSON response body\r",
                      "  const json = pm.response.json();\r",
                      "\r",
                      "  // Validate that accountId exists in the response\r",
                      "  pm.expect(json.accountId).to.exist;\r",
                      "\r",
                      "  // Store accountId in environment for request chaining\r",
                      "  pm.environment.set(\"billing_account_id\", json.accountId);\r",
                      "});\r",
                      "\r",
                      "\r",
                      "// ---------------------------------------------------------\r",
                      "// Contract Schema: Create Billing Account Response\r",
                      "// Purpose:\r",
                      "// - Define the expected response structure for account creation\r",
                      "// - Acts as a contract between PolicyCenter and BillingCenter\r",
                      "// - Prevents breaking changes to integration consumers\r",
                      "// ---------------------------------------------------------\r",
                      "const createAccountSchema = {\r",
                      "  type: \"object\",\r",
                      "\r",
                      "  // Required fields returned after account creation\r",
                      "  required: [\"accountId\", \"status\", \"currency\"],\r",
                      "\r",
                      "  properties: {\r",
                      "    // Unique identifier for the newly created billing account\r",
                      "    accountId: { type: \"string\" },\r",
                      "\r",
                      "    // Initial lifecycle status of the account (e.g., ACTIVE)\r",
                      "    status: { type: \"string\" },\r",
                      "\r",
                      "    // Currency associated with the billing account (e.g., CAD, USD)\r",
                      "    currency: { type: \"string\" }\r",
                      "  }\r",
                      "};\r",
                      "\r",
                      "\r",
                      "// ---------------------------------------------------------\r",
                      "// Test Case: Contract Validation\r",
                      "// Purpose:\r",
                      "// - Ensure the response conforms to the agreed API contract\r",
                      "// - Detect missing or renamed fields early\r",
                      "// - Protect downstream systems from integration failures\r",
                      "// ---------------------------------------------------------\r",
                      "pm.test(\"Create Billing Account contract valid\", () => {\r",
                      "  pm.response.to.have.jsonSchema(createAccountSchema);\r",
                      "});\r",
                      ""
                    ],
                    "type": "text/javascript",
                    "packages": {},
                    "requests": {}
                  }
                }
              ],
              "request": {
                "method": "POST",
                "header": [],
                "url": {
                  "raw": "{{bc_mock_base_url}}/v1/billing/accounts",
                  "host": [
                    "{{bc_mock_base_url}}"
                  ],
                  "path": [
                    "v1",
                    "billing",
                    "accounts"
                  ]
                }
              },
              "response": []
            }
          ]
        },
        {
          "name": "02_Get_Account",
          "item": [
            {
              "name": "v1 GET Billing Account - Success",
              "event": [
                {
                  "listen": "test",
                  "script": {
                    "exec": [
                      "// ---------------------------------------------------------\r",
                      "// Test Case: Retrieve Billing Account successfully\r",
                      "// Purpose:\r",
                      "// - Verify that the BillingCenter API responds correctly\r",
                      "// - Confirms the account exists and the request was processed\r",
                      "// ---------------------------------------------------------\r",
                      "pm.test(\"Account retrieved\", () => {\r",
                      "  // 200 OK indicates a successful retrieval of the billing account\r",
                      "  pm.response.to.have.status(200);\r",
                      "});\r",
                      "\r",
                      "\r",
                      "// ---------------------------------------------------------\r",
                      "// Test Case: Validate Billing Account Status\r",
                      "// Business Rule:\r",
                      "// - Retrieved billing accounts must be in ACTIVE state\r",
                      "// - PolicyCenter relies on this status before allowing billing actions\r",
                      "// ---------------------------------------------------------\r",
                      "pm.test(\"Account status is Active\", () => {\r",
                      "  // Parse JSON response body\r",
                      "  const json = pm.response.json();\r",
                      "\r",
                      "  // Validate business state of the billing account\r",
                      "  pm.expect(json.status).to.eql(\"Active\");\r",
                      "});\r",
                      "\r",
                      "\r",
                      "// ---------------------------------------------------------\r",
                      "// Contract Schema: Get Billing Account Response\r",
                      "// Purpose:\r",
                      "// - Defines the expected structure of the API response\r",
                      "// - Acts as a contract between PolicyCenter and BillingCenter\r",
                      "// - Prevents breaking changes in downstream integrations\r",
                      "// ---------------------------------------------------------\r",
                      "const getAccountSchema = {\r",
                      "  type: \"object\",\r",
                      "\r",
                      "  // Mandatory fields required for PolicyCenter integration\r",
                      "  required: [\"accountId\", \"policies\", \"balance\", \"status\"],\r",
                      "\r",
                      "  properties: {\r",
                      "    // Unique identifier for the billing account\r",
                      "    accountId: { type: \"string\" },\r",
                      "\r",
                      "    // List of policy numbers associated with this billing account\r",
                      "    policies: {\r",
                      "      type: \"array\",\r",
                      "      items: { type: \"string\" }\r",
                      "    },\r",
                      "\r",
                      "    // Current outstanding balance for the account\r",
                      "    balance: { type: \"number\" },\r",
                      "\r",
                      "    // Current lifecycle status of the account (e.g., Active, Cancelled)\r",
                      "    status: { type: \"string\" }\r",
                      "  }\r",
                      "};\r",
                      "\r",
                      "\r",
                      "// ---------------------------------------------------------\r",
                      "// Test Case: Contract Validation\r",
                      "// Purpose:\r",
                      "// - Ensures the response adheres to the agreed API contract\r",
                      "// - Catches missing or renamed fields early\r",
                      "// - Protects downstream systems from integration failures\r",
                      "// ---------------------------------------------------------\r",
                      "pm.test(\"Get Billing Account contract valid\", () => {\r",
                      "  pm.response.to.have.jsonSchema(getAccountSchema);\r",
                      "});\r",
                      ""
                    ],
                    "type": "text/javascript",
                    "packages": {},
                    "requests": {}
                  }
                }
              ],
              "request": {
                "method": "GET",
                "header": [],
                "url": {
